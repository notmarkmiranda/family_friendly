<div class="container">
  <h1>Search Results</h1>
  <div class="jumbotron">
    <%= @results.businesses.each do |b| %>
    <h3><%= b.name %></h3>
    <ul>
      <li><%= b.location.display_address %></li>
      <li><%= b.phone %></li>
      <li>Average Yelp Rating: <%= b.rating %> Stars</li>
    </ul>
    <% end %>
  </div>
</div>
